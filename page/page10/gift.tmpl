<div class="gift-menu-list clearfix">
	<%for(var i = 0, len = data.length; i < len; i++) {%>
	<div class="gift-menu-item" data-gift_id="<%=data[i].gift_id%>">
		<img src="<%=data[i].img_url%>">
		<div class="gift-menu-item-select">
			<span class="select-item"></span>
			<span class="gift-menu-item-title"><%=data[i].name%></span>
		</div>
	</div>
	<%}%>
</div>
<div class="wish-gift-submit" data-pageswitch='{"currentPage": "page10", "nextPage": "page7"}'>提交</div>